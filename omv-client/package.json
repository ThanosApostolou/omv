{
    "name": "omv-client",
    "version": "0.0.0",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "serve": "ng serve --open",
        "build": "npm run lint && ng build --prod",
        "electron": "electron .",
        "electron:build": "npm run build && electron-builder build",
        "android:build": "ng build --prod --output-path=cordova/www && cd cordova && cordova build android --release",
        "android": "cd cordova && cordova run android",
        "lint": "eslint --ext .html,.js,.jsx,.ts,.tsx src",
        "test": "ng test",
        "e2e": "ng e2e"
    },
    "build": {
        "appId": "com.github.ThanosApostolou.omv",
        "productName": "omv-client",
        "files": [
            "main.js",
            "dist/omv-client"
        ],
        "directories": {
            "output": "dist/electron-builder"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.0.7",
        "@angular/cdk": "^9.2.0",
        "@angular/common": "^9.0.7",
        "@angular/compiler": "^9.0.7",
        "@angular/core": "^9.0.7",
        "@angular/flex-layout": "^9.0.0-beta.29",
        "@angular/forms": "^9.0.7",
        "@angular/material": "^9.2.0",
        "@angular/platform-browser": "^9.0.7",
        "@angular/platform-browser-dynamic": "^9.0.7",
        "@angular/router": "^9.0.7",
        "flex-layout": "^1.0.1",
        "rxjs": "^6.5.5",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.900.7",
        "@angular/cli": "^9.0.7",
        "@angular/compiler-cli": "^9.0.7",
        "@angular/language-service": "^9.0.7",
        "@types/node": "^12.12.34",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "cordova": "^9.0.0",
        "electron": "^8.2.0",
        "electron-builder": "^22.4.1",
        "eslint": "^6.8.0",
        "eslint-plugin-html": "^6.0.0",
        "eslint-plugin-import": "^2.20.2",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.7.5"
    }
}
